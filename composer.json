    "description": "Send free sms messages from an email account",
    "keywords": [
      "emailsms", "text", "sms", "email", "free"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "John Fowler",
            "email": "fowlergac@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
      "php": ">=5.4",
      "illuminate/mail": "5.5.*",
      "illuminate/log": "5.5.*"
    },
    "require-dev": {
        "php": ">=5.4.0",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "illuminate/console": "~5.0",
        "illuminate/support": "~5.0"
    },
    "autoload": {
        "psr-4": {
          "Larablocks\\EmailSMS\\": "src/EmailSMS/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "Larablocks\\EmailSMS\\PHPUnit\\": "tests/"
        }
    }
}
